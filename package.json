{
  "name": "ts-guardian",
  "version": "0.0.9",
  "description": "Functional, composable, reliable type guards",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.tsx\"",
    "prepare": "yarn build",
    "prepublishOnly": "yarn format",
    "test": "jest --config jestconfig.json --watch"
  },
  "keywords": [
    "type",
    "predicate",
    "typescript",
    "guard",
    "guards",
    "functional",
    "composable"
  ],
  "author": "Jonathan Madelaine",
  "license": "Unlicense",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmadelaine/ts-guardian.git"
  }
}
